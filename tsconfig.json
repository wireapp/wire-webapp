{
  "compilerOptions": {
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "lib": ["dom", "esnext"],
    "jsx": "react-jsx",
    "module": "commonjs",
    "moduleResolution": "node",
    "noEmitOnError": false,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "removeComments": false,
    "sourceMap": false,
    "strict": true,
    "target": "es2017",
    "declaration": true,
    "declarationMap": true
  }
}
