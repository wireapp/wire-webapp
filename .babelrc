{
  "plugins": [
    "transform-object-rest-spread",
    "transform-class-properties",
    [
      "react-intl",
      {
        "messagesDir": "./app/i18n"
      }
    ]
  ],
  "presets": [
    "react",
    [
      "env",
      {
        "debug": true,
        "modules": false,
        "targets": {
          "browsers": ["chrome >= 56", "firefox >= 52", "edge >= 14", "opera >= 43"]
        },
        "useBuiltIns": true
      }
    ]
  ]
}
