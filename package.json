    "build": "yarn build:node && yarn build:browser",
    "build:browser": "webpack --config ./webpack.browser.js",
    "build:node": "tsc",
    "clear": "rimraf dist",
    "coverage": "istanbul cover --report html ./node_modules/jasmine/bin/jasmine.js",
