  "engines": {
    "yarn": ">= 1.0.0"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix",
      "git add"
    ],
    "*.{json,md}": [
      "prettier --write",
      "git add"
    ],
    "*.less": [
      "prettier --write",
      "stylelint --fix",
      "git add"
    ],
    "src/script/**/*.{ts,tsx}": [
      "tslint --config tslint.json --project tsconfig.json --fix",
      "git add"
    ],
    "server/**/*.{ts,tsx}": [
      "tslint --config server/tslint.json",
      "git add"
    ]
  },
  "prettier": "@wireapp/prettier-config",
  "scripts": {
    "build:server": "cd server && yarn build",
    "bundle:dev": "webpack --config webpack.config.dev.js --progress",
    "bundle:prod": "webpack --config webpack.config.js",
    "bundle:test": "webpack --config webpack.config.test.js",
    "build:prod": "yarn build:server && grunt build_prod",
    "configure": "copy-config",
    "deploy": "yarn && yarn build:prod && eb deploy",
    "dist": "grunt build",
    "docker": "node bin/push_docker.js",
    "fix": "yarn fix:assets && yarn fix:js && yarn fix:ts",
    "fix:assets": "yarn prettier --write && yarn stylelint --fix",
    "fix:js": "yarn lint:js --fix",
    "fix:ts": "yarn lint:ts:app --fix && yarn fix:ts:server",
    "fix:ts:server": "cd server && yarn fix",
    "lint": "yarn lint:assets && yarn lint:js && yarn lint:ts:app",
    "lint:assets": "yarn prettier --list-different && yarn stylelint",
    "lint:js": "eslint -c .eslintrc.json --ignore-path .gitignore --ignore-path .eslintignore --ext .js src/script/ test/ bin/ grunt/",
    "lint:ts": "yarn lint:ts:app && yarn lint:ts:server",
    "lint:ts:app": "tslint --config tslint.json --project tsconfig.json \"src/script/**/*.{ts,tsx}\"",
    "lint:ts:server": "cd server && yarn lint",
    "postinstall": "yarn configure && grunt init && cd server && yarn",
    "prettier": "prettier --ignore-path .gitignore \"**/*.{json,md}\"",
    "start": "yarn build:server && grunt build && concurrently \"yarn start:server:dev\" \"grunt watch\"",
    "start:server:dev": "cross-env NODE_ENV=development node ./server/dist/index.js",
    "stylelint": "prettier --ignore-path .gitignore --write \"**/*.less\" && stylelint --ignore-path .gitignore \"**/*.less\"",
    "build:style": "node ./bin/less.js",
    "test": "yarn test:types && yarn lint && yarn test:server && yarn test:auth && yarn test:app",
    "test:app": "node --max-old-space-size=4096 ./node_modules/karma/bin/karma start karma.conf.js",
    "test:auth": "yarn bundle:test && karma start karma.conf.react.js",
    "test:server": "cd server && yarn test",
    "test:types": "tsc --noEmit",
    "translate:upload": "yarn translate:merge && node ./bin/translations_upload.js",
    "translate:merge": "yarn bundle:prod && node ./bin/translations_normalize.js",
    "translate:extract": "i18next-scanner 'src/{page,script}/**/*.{js,html,htm}'"
  },
  "version": "0.1.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/wireapp/wire-webapp.git"
  },
  "exportsOverride": {
    "@bower_components/normalize-css": {
      "css": "normalize.css"
    },
    "@bower_components/webapp-module-bubble": {
      "js": "dist/script/*.js",
      "less": "app/style/webapp-module-bubble.less"
    },
    "@bower_components/webapp-module-namespace": {
      "js": "js/*.js"
    },
    "@bower_components/wire-theme": {
      "less": "app/style/{wire-theme,variables,input}.less"
    }
  }
}
